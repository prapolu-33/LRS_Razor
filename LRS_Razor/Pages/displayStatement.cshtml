@page
@model LRS_Razor.Pages.displayStatementModel
@{
    ViewData["Title"] = "Display";
}

<h1>Display</h1>

<table class="table">
    <thead>
        <tr>
            <!-- Define table headers for your columns -->
            <th>Column1</th>
            <th>Column2</th>
            <th>Column3</th>
            <th>Column4</th>
            <th>Column5</th>
            <th>Column6</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var record in Model.rows)
        {
            <tr>
                <td>@record.Uuid</td>
                <td>@record.Name</td>
                <td>@record.Verb</td>
                <td>@record.Object</td>
                <td>@record.TimeStamp</td>
            </tr>
        }
    </tbody>
</table>